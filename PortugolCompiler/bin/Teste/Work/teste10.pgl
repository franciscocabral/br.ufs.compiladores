inicio
   
   inteiro: anterior, atual, proximo;

   anterior <- 0;      //Valor anterior
   atual <- 1;         //Valor atual
   proximo <- 1;        //Proximo valor
   
   // Sequencia de Fibonacci
   enquanto (atual < 10) faca 
       proximo <- anterior + atual; //ExpressÃ£o soma de anterior + atual = proximo
       escreva(proximo); 
       anterior <- atual; 
       atual <- proximo; 
   fim enquanto;
 
fim.