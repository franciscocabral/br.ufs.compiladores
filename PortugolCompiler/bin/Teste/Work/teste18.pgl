programa teste18
inicio
   
   inteiro: x,y;
   leia(x,y);
   
   se ((x <> 0) e (y <> 0)) entao
     escreva(x);
   fim se;
     
   se  (x <> 0 ou y <> 0) entao
     escreva(y);  
   fim se;
     
   se  nao (x = 0 ou y = 0) entao
      escreva(x);
   fim se;
   
   se  nao (x = 0 e y = 0) entao
      escreva(x);
   fim se;
      
fim.
