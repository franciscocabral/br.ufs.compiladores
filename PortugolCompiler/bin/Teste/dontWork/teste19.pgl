
programa teste19
inicio
   
   inteiro: anterior, atual, prox_val;

   anterior <- 0;      //Valor anterior
   atual <- 1;         //Valor atual
   prox_val <- 1;        //Proximo valor
   
   // Sequencia de Fibonacci
   enquanto (atual < 10) faca 
       prox_val <- anterior + atual; //ExpressÃ£o soma de anterior + atual = proxima
       escreva(prox_val); 
       anterior <- atual; 
       atual <- pro_val; 
   fim enquanto;
 